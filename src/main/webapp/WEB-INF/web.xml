<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>SMS</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>smsUserServlet</servlet-name>
		<servlet-class>smsUserServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>smsUserServlet</servlet-name>
		<url-pattern>/smsUserServlet/dashboard</url-pattern>
		<url-pattern>/smsUserServlet/edit</url-pattern>
		<url-pattern>/smsUserServlet/update</url-pattern>
		<url-pattern>/smsUserServlet/delete</url-pattern>
		<url-pattern>/smsUserServlet/insert</url-pattern>
		<url-pattern>/smsUserServlet/logout</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>smsLogoutServlet</servlet-name>
		<servlet-class>com.example.smsLogoutServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>smsLogoutServlet</servlet-name>
		<url-pattern>/smsLogoutServlet</url-pattern>
	</servlet-mapping>

</web-app>